*color10: #ABAE8F

!yellow
*color3:  #F1DD9E
*color11: #F1DD9E

!blue
*color4:  #BBDBCA
*color12: #BBDBCA

!magenta
*color5:  #D2A5C9
*color13: #D2A5C9

!cyan
*color6:  #C2DED9
*color14: #C2DED9

!white
*color7:  #F4F4F2
