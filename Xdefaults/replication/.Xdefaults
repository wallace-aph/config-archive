URxvt.depth: 32
URxvt.scrollBar: false
URxvt.scrollstyle: plain
URxvt.font: xft:Inconsolata:size=18
!URxvt.boldFont: xft:Monaco:size=18:bold
# URxvt.boldFont: xft:berry
# URxvt.font: xft:berry

!urxvt*transparent: true
URxvt*termName: rxvt-unicode-256color
URxvt*geometry: 80x25
URxvt*internalBorder:  10
URxvt.letterSpace: -1

*foreground: #D7D0C7

URxvt.background: rgba:1111/1111/1111/eeee

URxvt*.shading: 5
URxvt*.blurRadius: 200

